digraph AFN {
	rankdir=LR
	A [shape=doublecircle]
	B [shape=circle]
	C [shape=circle]
	D [shape=doublecircle]
	E [shape=circle]
	F [shape=circle]
	"" [height=0 shape=none width=0]
	"" -> F
	G [shape=circle]
	H [shape=circle]
	H -> E [label=a]
	H -> G [label=b]
	H -> C [label=a]
	H -> A [label=a]
	H -> H [label=a]
	H -> D [label=b]
	H -> B [label=c]
	H -> A [label=b]
	H -> B [label=c]
	H -> A [label=b]
	H -> B [label=c]
	H -> D [label=b]
	H -> D [label=b]
}
