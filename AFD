digraph AFN {
	rankdir=LR
	A [shape=doublecircle]
	"" [height=0 shape=none width=0]
	"" -> A
	B [shape=circle]
	C [shape=circle]
	D [shape=doublecircle]
	E [shape=doublecircle]
	A -> B [label=0]
	A -> C [label=1]
	C -> D [label=0]
	D -> C [label=1]
	B -> E [label=1]
	E -> B [label=0]
	E -> C [label=1]
}
